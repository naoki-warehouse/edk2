; Module Name:
;
; DivU64x32.nasm
;
; Abstract:
;
; Calculate the quotient of a 64-bit integer by a 32-bit integer
;
;------------------------------------------------------------------------------

    SECTION .text

;------------------------------------------------------------------------------
; UINT64
; __attribute__((cdecl))
; InternalMathDivU64x32 (
; UINT64 Dividend,
; UINT32 Divisor
; );
;------------------------------------------------------------------------------
global InternalMathDivU64x32
InternalMathDivU64x32:
    mov eax, [esp + 8]
    mov ecx, [esp + 12]
    xor edx, edx
    div ecx
    push eax ; save quotient on stack
    mov eax, [esp + 8]
    div ecx
    pop edx ; restore high-order dword of the quotient
    ret
